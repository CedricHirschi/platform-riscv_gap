{
  "build": {
    "extra_flags": "-D__GAP8__",
    "f_cpu": "250000000L",
    "ldscript": "GAP8.ld",
    "march": "rv32imcxgap8",
    "target": "GWT",
    "variant": "GAP8"
  },
  "debug": {
    "tools": {
      "ftdi": {
        "init_cmds": [
          "define pio_reset_halt_target",
          "    monitor reset halt",
          "end",
          "define pio_reset_target",
          "    monitor reset",
          "end",
          "target remote $DEBUG_PORT",
          "$INIT_BREAK",
          "$LOAD_CMD",
          "pio_reset_halt_target"
        ],
        "load_cmd": " ",
        "onboard": true,
        "port": ":1234",
        "server": {
          "arguments": [
            "bin/plpbridge",
            "--verbose=3",
            "--chip=gap",
            "--cable=ftdi@digilent",
            "--debug",
            "--boot-mode=jtag",
            "--binary",
            "$PROG_PATH",
            "ioloop",
            "start",
            "gdb",
            "wait"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pulp-debug-bridge"
        }
      }
    }
  },
  "frameworks": [
    "mbed",
    "pulp-os"
  ],
  "name": "GAPUINO GAP8 development board",
  "upload": {
    "maximum_ram_size": 8388608,
    "maximum_size": 67108864,
    "protocol": "ftdi"
  },
  "url": "https://greenwaves-technologies.com/product/gapduino/",
  "vendor": "GreenWaves Technologies"
}